@model IList<BlogArtikel>

@{
    ViewData["Title"] = "BlogArticle list";
}

<table class="table table-striped">
    <thead>
        <tr>
            <th>id</th>
            <th>title</th>
            <th>description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var blogArtikel in Model)
        {
            <partial name="BlogArtikelRowPartial" model="blogArtikel" />
        }
    </tbody>
</table>